{
  "cosmos-installer": {
    "routes": [
        {
          "name": "LibreTranslate",
          "description": "Web UI",
          "useHost": true,
          "target": "http://libretranslate:5000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "libretranslate": {
  "container_name": "big-bear-libretranslate",
  "image": "libretranslate/libretranslate:v1.7.3",
  "restart": "unless-stopped",
  "environment": [
    "LT_API_KEYS=true",
    "LT_API_KEYS_DB_PATH=/app/db/api_keys.db",
    "LT_UPDATE_MODELS=true",
    "LT_LOAD_ONLY=en,fr,es,de,it,pt,ru,ja,zh"
  ],
  "volumes": [
    "libretranslate_db:/app/db",
    "libretranslate_.local:/home/libretranslate/.local:rw"
  ],
  "ports": [
    "5000:5000"
  ],
  "tty": true,
  "healthcheck": {
    "test": [
      "CMD-SHELL",
      "./venv/bin/python scripts/healthcheck.py"
    ]
  }
}
    }
  }
}
