{
  "cosmos-installer": {
    "routes": [
        {
          "name": "WatchYourLAN",
          "description": "Web UI",
          "useHost": true,
          "target": "http://watchyourlan:8840",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "watchyourlan": {
  "container_name": "big-bear-watchyourlan",
  "image": "aceberg/watchyourlan:2.1.4",
  "environment": [
    "IFACES=\"eth0\"",
    "TZ=\"UTC\""
  ],
  "restart": "unless-stopped",
  "volumes": [
    "watchyourlan_data:/data/WatchYourLAN"
  ],
  "network_mode": "host"
}
    }
  }
}
