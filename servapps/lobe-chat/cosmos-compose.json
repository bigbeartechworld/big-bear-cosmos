{
  "cosmos-installer": {
    "routes": [
        {
          "name": "LobeChat",
          "description": "Web UI",
          "useHost": true,
          "target": "http://lobe-chat:3210",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "lobe-chat": {
  "container_name": "big-bear-lobe-chat",
  "image": "lobehub/lobe-chat:1.142.7",
  "restart": "unless-stopped",
  "environment": [
    "OPENAI_API_KEY=\"\"",
    "OPENAI_PROXY_URL=\"\"",
    "CUSTOM_MODELS=\"\"",
    "ACCESS_CODE=\"\""
  ],
  "ports": [
    "3210:3210"
  ]
}
    }
  }
}
