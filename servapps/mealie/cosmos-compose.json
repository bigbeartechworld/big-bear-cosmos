{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Mealie",
          "description": "Web UI",
          "useHost": true,
          "target": "http://mealie:9925",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "mealie": {
  "container_name": "big-bear-mealie",
  "image": "hkotel/mealie:v3.7.0",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-mealie_data:/app/data"
  ],
  "ports": [
    "9925:9000"
  ]
}
    }
  }
}
