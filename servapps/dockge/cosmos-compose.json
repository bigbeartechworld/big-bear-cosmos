{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "louislam/dockge:1.5.0",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "DOCKGE_STACKS_DIR=/DATA/AppData/big-bear-dockge/stacks"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/dockge/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/dockge/var/run/docker.sock",
          "target": "/var/run/docker.sock",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/dockge/DATA/AppData/$AppID/data",
          "target": "/app/data",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/dockge/DATA/AppData/$AppID/stacks",
          "target": "/DATA/AppData/big-bear-dockge/stacks",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:5001",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
