{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Penpot",
          "description": "Web UI",
          "useHost": true,
          "target": "http://penpot:9001",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "penpot": {
  "container_name": "big-bear-penpot-frontend",
  "image": "penpotapp/frontend:2.10.1",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-penpot_assets:/opt/data/assets"
  ],
  "ports": [
    "9001:8080"
  ],
  "depends_on": [
    "penpot-backend",
    "penpot-exporter"
  ],
  "networks": [
    "big-bear-penpot-network"
  ]
}
    }
  }
}
