{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Audiobookshelf",
          "description": "Web UI",
          "useHost": true,
          "target": "http://audiobookshelf:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "audiobookshelf": {
  "container_name": "big-bear-audiobookshelf",
  "image": "ghcr.io/advplyr/audiobookshelf:2.30.0",
  "restart": "unless-stopped",
  "volumes": [
    "audiobookshelf_media_data_books_spoken:/audiobooks",
    "audiobookshelf_media_data_podcasts:/podcasts",
    "audiobookshelf_data_config:/config",
    "audiobookshelf_data_metadata:/metadata"
  ],
  "ports": [
    "8080:80"
  ]
}
    }
  }
}
