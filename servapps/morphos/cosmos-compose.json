{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Morphos",
          "description": "Web UI",
          "useHost": true,
          "target": "http://morphos:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "morphos": {
  "container_name": "big-bear-morphos",
  "image": "ghcr.io/danvergara/morphos-server:0.6.0",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-morphos_tmp:/tmp"
  ],
  "ports": [
    "8080:8080"
  ],
  "healthcheck": {
    "test": "timeout 10s bash -c ':> /dev/tcp/127.0.0.1/8080' || exit 1",
    "interval": "60s",
    "retries": 3,
    "start_period": "20s",
    "timeout": "30s"
  }
}
    }
  }
}
