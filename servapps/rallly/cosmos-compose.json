{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Rallly",
          "description": "Web UI",
          "useHost": true,
          "target": "http://rallly:3000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "rallly": {
  "container_name": "big-bear-rallly",
  "image": "lukevella/rallly:4.4.1",
  "restart": "unless-stopped",
  "environment": [
    "DATABASE_URL=postgres://rallly:f87c7126-f816-47ef-9193-2e5eec763fcd@big-bear-rallly-db/big-bear-rallly",
    "SECRET_PASSWORD=0f6f292d-c78c-4709-920d-e3b6361eb5b5",
    "NEXT_PUBLIC_BASE_URL=http://[YOUR_CASAOS_IP]:3000",
    "NOREPLY_EMAIL=noreply@example.com",
    "SUPPORT_EMAIL=support@example.com",
    "SMTP_HOST=[YOUR_CASAOS_IP]",
    "SMTP_PORT=1025",
    "SMTP_SECURE=",
    "SMTP_USER=",
    "SMTP_PWD=",
    "SMTP_TLS_ENABLED=",
    "ALLOWED_EMAILS="
  ],
  "ports": [
    "3000:3000"
  ],
  "networks": [
    "big_bear_rally_network"
  ],
  "depends_on": {
    "big-bear-rallly-db": {
      "condition": "service_healthy"
    }
  }
}
    }
  }
}
