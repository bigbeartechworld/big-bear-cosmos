{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Budibase",
          "description": "Web UI",
          "useHost": true,
          "target": "http://budibase:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "budibase": {
  "container_name": "big-bear-budibase",
  "image": "budibase/budibase:3.22.6",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-budibase_data:/data"
  ],
  "ports": [
    "8080:80"
  ]
}
    }
  }
}
