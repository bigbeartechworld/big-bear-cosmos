{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "ghcr.io/cmintey/wishlist:v0.51.1",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "ORIGIN=",
        "TOKEN_TIME=72",
        "DEFAULT_CURRENCY=USD",
        "LOG_LEVEL=info"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/wishlist/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/wishlist/DATA/AppData/$AppID/uploads",
          "target": "/usr/src/app/uploads",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/wishlist/DATA/AppData/$AppID/data",
          "target": "/usr/src/app/data",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:3280",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
