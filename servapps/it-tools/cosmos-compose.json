{
  "cosmos-installer": {
    "routes": [
        {
          "name": "IT Tools",
          "description": "Web UI",
          "useHost": true,
          "target": "http://it-tools:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "it-tools": {
  "container_name": "big-bear-it-tools",
  "image": "corentinth/it-tools:2023.11.2-7d94e11",
  "restart": "unless-stopped",
  "ports": [
    "8080:80"
  ]
}
    }
  }
}
