{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Baserow",
          "description": "Web UI",
          "useHost": true,
          "target": "http://baserow:7350",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "baserow": {
  "container_name": "baserow",
  "image": "baserow/baserow:1.35.3",
  "environment": {
    "BASEROW_PUBLIC_URL": "http://[YOUR_CASAOS_IP]:7350"
  },
  "ports": [
    "7350:80",
    "7351:443"
  ],
  "volumes": [
    "big-bear-baserow_data:/baserow/data"
  ]
}
    }
  }
}
