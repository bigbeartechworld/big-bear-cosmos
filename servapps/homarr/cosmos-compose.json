{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Homarr",
          "description": "Web UI",
          "useHost": true,
          "target": "http://homarr:7575",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "homarr": {
  "container_name": "homarr",
  "image": "ghcr.io/ajnart/homarr:0.16.1",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-homarr_configs:/app/data/configs",
    "big-bear-homarr_icons:/app/public/icons",
    "big-bear-homarr_data:/data",
    "/var/run/docker.sock:/var/run/docker.sock"
  ],
  "ports": [
    "7575:7575"
  ]
}
    }
  }
}
