{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "ghcr.io/ajnart/homarr:0.16.0",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/homarr/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/homarr/DATA/AppData/$AppID/configs",
          "target": "/app/data/configs",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/homarr/DATA/AppData/$AppID/icons",
          "target": "/app/public/icons",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/homarr/DATA/AppData/$AppID/data",
          "target": "/data",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/homarr/var/run/docker.sock",
          "target": "/var/run/docker.sock",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:7575",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
