{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Jlesage Firefox",
          "description": "Web UI",
          "useHost": true,
          "target": "http://jlesage-firefox:5800",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "jlesage-firefox": {
  "container_name": "big-bear-jlesage-firefox",
  "image": "jlesage/firefox:v25.09.1",
  "ports": [
    "5800:5800"
  ],
  "environment": [
    "VNC_PASSWORD=casaos",
    "FF_OPEN_URL=https://community.bigbeartechworld.com/"
  ],
  "volumes": [
    "big-bear-jlesage-firefox_config:/config:rw"
  ]
}
    }
  }
}
