{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Minio",
          "description": "Web UI",
          "useHost": true,
          "target": "http://minio:9011",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "minio": {
  "image": "bigbeartechworld/big-bear-minio:RELEASE.2025-10-15T17-29-55Z",
  "container_name": "big-bear-minio",
  "volumes": [
    "/DATA/AppData/$AppID:/data"
  ],
  "ports": [
    "9010:9000",
    "9011:9001"
  ],
  "environment": {
    "MINIO_ROOT_PASSWORD": "minioadmin",
    "MINIO_ROOT_USER": "minioadmin"
  },
  "restart": "unless-stopped",
  "command": [
    "server",
    "--console-address",
    ":9001",
    "/data"
  ]
}
    }
  }
}
