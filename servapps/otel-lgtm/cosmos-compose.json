{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "grafana/otel-lgtm:0.11.12",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/otel-lgtm/icon.png"
      },
      "volumes": [
        {
          "source": "",
          "target": "",
          "type": "volume"
        },
        {
          "source": "{ServiceName}-type",
          "target": " bind",
          "type": "volume"
        },
        {
          "source": "{ServiceName}-source",
          "target": " /DATA/AppData/$AppID/data",
          "type": "volume"
        },
        {
          "source": "{ServiceName}-target",
          "target": " /data",
          "type": "volume"
        },
        {
          "source": "",
          "target": "",
          "type": "volume"
        },
        {
          "source": "{ServiceName}-  create_host_path",
          "target": " true",
          "type": "volume"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:3000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
