{
  "cosmos-installer": {
    "routes": [
        {
          "name": "EJBCA Community",
          "description": "Web UI",
          "useHost": true,
          "target": "http://ejbca-ce:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "ejbca-ce": {
  "container_name": "big-bear-ejbca-ce",
  "image": "keyfactor/ejbca-ce:9.1.1",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-ejbca-ce_persistent:/mnt/persistent"
  ],
  "ports": [
    "8080:8080",
    "8443:8443"
  ],
  "environment": [
    "TLS_SETUP_ENABLED=simple"
  ]
}
    }
  }
}
