{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Anse",
          "description": "Web UI",
          "useHost": true,
          "target": "http://anse:8014",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "anse": {
  "container_name": "big-bear-anse",
  "image": "ddiu8081/anse:v1.1.11",
  "restart": "unless-stopped",
  "environment": [
    "PORT=8014"
  ],
  "ports": [
    "8014:8014"
  ]
}
    }
  }
}
