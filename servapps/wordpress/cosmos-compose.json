{
  "cosmos-installer": {
    "routes": [
        {
          "name": "WordPress",
          "description": "Web UI",
          "useHost": true,
          "target": "http://wordpress:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "wordpress": {
  "image": "wordpress:6.9.1",
  "ports": [
    "8080:80"
  ],
  "environment": {
    "WORDPRESS_DB_HOST": "db",
    "WORDPRESS_DB_USER": "casaos",
    "WORDPRESS_DB_PASSWORD": "casaos",
    "WORDPRESS_DB_NAME": "wordpress"
  },
  "volumes": [
    "big-bear-wordpress_html:/var/www/html"
  ],
  "depends_on": [
    "db"
  ]
}
    }
  }
}
