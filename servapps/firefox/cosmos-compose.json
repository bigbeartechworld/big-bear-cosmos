{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Firefox",
          "description": "Web UI",
          "useHost": true,
          "target": "http://firefox:6902",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "firefox": {
  "container_name": "big-bear-firefox",
  "image": "kasmweb/firefox:1.15.0-rolling",
  "ports": [
    "6902:6901"
  ],
  "environment": [
    "VNC_PW=casaos",
    "LAUNCH_URL=https://community.bigbeartechworld.com/"
  ],
  "shm_size": "512m",
  "volumes": [
    "firefox_Downloads:/home/kasm-user/Downloads"
  ]
}
    }
  }
}
