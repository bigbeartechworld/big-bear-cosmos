{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Firefox",
          "description": "Web UI",
          "useHost": true,
          "target": "http://firefox:3000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "firefox": {
  "container_name": "big-bear-firefox",
  "image": "linuxserver/firefox:1143.0.4",
  "ports": [
    "3000:3000",
    "3001:3001"
  ],
  "environment": [
    "PUID=1000",
    "PGID=1000",
    "TZ=Etc/UTC",
    "FIREFOX_CLI=https://community.bigbeartechworld.com/"
  ],
  "shm_size": "1gb",
  "volumes": [
    "firefox_config:/config"
  ]
}
    }
  }
}
