{
  "cosmos-installer": {
    "routes": [
        {
          "name": "RustFS",
          "description": "Web UI",
          "useHost": true,
          "target": "http://rustfs:9000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "rustfs": {
  "image": "rustfs/rustfs:1.0.0-alpha.71",
  "container_name": "rustfs",
  "ports": [
    "9000:9000",
    "9001:9001"
  ],
  "volumes": [
    "big-bear-rustfs_data:/data",
    "big-bear-rustfs_logs:/logs"
  ],
  "environment": [
    "RUSTFS_ACCESS_KEY=bigbear",
    "RUSTFS_SECRET_KEY=f1805acc-55a7-4964-849a-8076b3e1cfdf",
    "RUSTFS_CONSOLE_ENABLE=true",
    "RUSTFS_ADDRESS=0.0.0.0:9000",
    "RUSTFS_CONSOLE_ADDRESS=0.0.0.0:9001",
    "TZ=UTC"
  ],
  "restart": "unless-stopped",
  "healthcheck": {
    "test": [
      "CMD",
      "sh",
      "-c",
      "curl -f http://localhost:9000/health && curl -f http://localhost:9001/rustfs/console/health"
    ],
    "interval": "30s",
    "timeout": "10s",
    "retries": 3,
    "start_period": "40s"
  }
}
    }
  }
}
