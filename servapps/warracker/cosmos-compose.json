{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "ghcr.io/sassanix/warracker/main:9.9.3",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "DB_HOST=big-bear-warrackerdb",
        "DB_NAME=big_bear_warracker",
        "DB_USER=big_bear_warracker_user",
        "DB_PASSWORD=4892f1bc-f1ea-464f-b528-1d52b2c9bb05",
        "SMTP_HOST=smtp.email.com",
        "SMTP_PORT=465",
        "SMTP_USERNAME=youremail@email.com",
        "SMTP_PASSWORD=password"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/warracker/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/warracker/DATA/AppData/$AppID/uploads",
          "target": "/data/uploads",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:80",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
