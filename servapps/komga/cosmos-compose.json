{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Komga",
          "description": "Web UI",
          "useHost": true,
          "target": "http://komga:25600",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "komga": {
  "container_name": "big-bear-komga",
  "image": "ghcr.io/gotson/komga:1.23.6",
  "restart": "unless-stopped",
  "environment": [
    "TZ=UTC"
  ],
  "volumes": [
    "big-bear-komga_config:/config",
    "big-bear-komga_data:/data"
  ],
  "ports": [
    "25600:25600"
  ]
}
    }
  }
}
