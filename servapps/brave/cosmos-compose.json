{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Brave",
          "description": "Web UI",
          "useHost": true,
          "target": "http://brave:6901",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "brave": {
  "container_name": "big-bear-brave",
  "image": "kasmweb/brave:1.15.0-rolling",
  "ports": [
    "6901:6901"
  ],
  "environment": [
    "VNC_PW=casaos",
    "LAUNCH_URL=https://community.bigbeartechworld.com/"
  ],
  "shm_size": "512m",
  "volumes": [
    "big-bear-brave_Downloads:/home/kasm-user/Downloads"
  ]
}
    }
  }
}
