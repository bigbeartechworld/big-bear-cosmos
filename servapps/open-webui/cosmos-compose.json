{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Open WebUI",
          "description": "Web UI",
          "useHost": true,
          "target": "http://open-webui:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "open-webui": {
  "container_name": "big-bear-open-webui",
  "image": "ghcr.io/open-webui/open-webui:git-33e54a9",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-open-webui_data:/app/backend/data"
  ],
  "environment": [
    "OLLAMA_BASE_URL=",
    "OPENAI_API_KEY="
  ],
  "ports": [
    "8080:8080"
  ]
}
    }
  }
}
