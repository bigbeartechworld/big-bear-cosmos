{
  "cosmos-installer": {
    "routes": [
        {
          "name": "What's Up Docker",
          "description": "Web UI",
          "useHost": true,
          "target": "http://whats-up-docker:3000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "whats-up-docker": {
  "image": "fmartinou/whats-up-docker:6.6.1",
  "container_name": "wud",
  "volumes": [
    "/var/run/docker.sock:/var/run/docker.sock"
  ],
  "ports": [
    "3000:3000"
  ]
}
    }
  }
}
