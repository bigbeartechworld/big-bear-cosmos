{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "linuxserver/code-server:4.105.1",
      "container_name": "{ServiceName}",
      "restart": "always",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "PUID=1000",
        "PGID=1000",
        "TZ=UTC",
        "PASSWORD=my_password",
        "SUDO_PASSWORD=sudo_my_password",
        "DEFAULT_WORKSPACE=/config/workspace"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/code-server/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/code-server/DATA/AppData/$AppID/config",
          "target": "/config",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/code-server/DATA/AppData/$AppID/projects",
          "target": "/projects",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:8443",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
