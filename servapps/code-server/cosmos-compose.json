{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Code Server",
          "description": "Web UI",
          "useHost": true,
          "target": "http://code-server:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "code-server": {
  "image": "linuxserver/code-server:4.108.2",
  "container_name": "code-server",
  "environment": [
    "PUID=1000",
    "PGID=1000",
    "TZ=UTC",
    "PASSWORD=my_password",
    "SUDO_PASSWORD=sudo_my_password",
    "DEFAULT_WORKSPACE=/config/workspace"
  ],
  "volumes": [
    "big-bear-code-server_config:/config",
    "big-bear-code-server_projects:/projects"
  ],
  "ports": [
    "8080:8443"
  ],
  "restart": "always"
}
    }
  }
}
