{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Stirling-PDF",
          "description": "Web UI",
          "useHost": true,
          "target": "http://stirling-pdf:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "stirling-pdf": {
  "container_name": "big-bear-stirling-pdf",
  "image": "stirlingtools/stirling-pdf:2.3.0",
  "restart": "unless-stopped",
  "environment": [
    "DOCKER_ENABLE_SECURITY=false",
    "SECURITY_ENABLE_LOGIN=true",
    "SECURITY_INITIALLOGIN_USERNAME=bigbear",
    "SECURITY_INITIALLOGIN_PASSWORD=4500733e-a0f8-4605-a712-fd267404956a",
    "CSRF_DISABLED=false",
    "DEFAULT_LOCALE=en-US",
    "INSTALL_BOOK_AND_ADVANCED_HTML_OPS=false",
    "LANGS=en_US"
  ],
  "volumes": [
    "big-bear-stirling-pdf_tessdata:/usr/share/tessdata",
    "big-bear-stirling-pdf_configs:/configs"
  ],
  "ports": [
    "8080:8080"
  ]
}
    }
  }
}
