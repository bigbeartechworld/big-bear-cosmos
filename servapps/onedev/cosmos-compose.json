{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "1dev/server:13.0.9",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "hibernate_dialect=io.onedev.server.persistence.PostgreSQLDialect",
        "hibernate_connection_driver_class=org.postgresql.Driver",
        "hibernate_connection_url=jdbc:postgresql://big-bear-onedev-db:5432/onedev",
        "hibernate_connection_username=bigbear",
        "hibernate_connection_password=eea2dad6-2203-45eb-8908-e8ba4d1e7f92",
        "initial_user=bigbear",
        "initial_password=password",
        "initial_email=admin@example.com",
        "initial_server_url=http://localhost:6610"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/onedev/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/onedev/DATA/AppData/$AppID/data",
          "target": "/opt/onedev",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/onedev/var/run/docker.sock",
          "target": "/var/run/docker.sock",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:6610",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
