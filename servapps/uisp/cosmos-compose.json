{
  "cosmos-installer": {
    "routes": [
        {
          "name": "UISP",
          "description": "Web UI",
          "useHost": true,
          "target": "http://uisp:10443",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "uisp": {
  "container_name": "big-bear-uisp",
  "image": "nico640/docker-unms:2.4.155.1",
  "restart": "unless-stopped",
  "environment": [
    "TZ=UTC"
  ],
  "volumes": [
    "uisp_config:/config"
  ],
  "ports": [
    "8080:80",
    "10443:443"
  ]
}
    }
  }
}
