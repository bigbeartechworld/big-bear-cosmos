{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Kavita",
          "description": "Web UI",
          "useHost": true,
          "target": "http://kavita:5000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "kavita": {
  "container_name": "big-bear-kavita",
  "image": "jvmilazz0/kavita:0.8.8",
  "restart": "unless-stopped",
  "volumes": [
    "kavita_data_kavita-config:/kavita/config",
    "kavita_media_data_books:/books",
    "kavita_media_data_comics:/comics",
    "kavita_media_data_manga:/manga"
  ],
  "ports": [
    "5000:5000"
  ]
}
    }
  }
}
