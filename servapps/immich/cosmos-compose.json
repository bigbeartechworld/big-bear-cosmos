{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "ghcr.io/immich-app/immich-server:v2.1.0",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "DB_HOSTNAME=immich-postgres",
        "DB_USERNAME=casaos",
        "DB_PASSWORD=casaos",
        "DB_DATABASE_NAME=immich",
        "DB_PORT=5432",
        "REDIS_HOSTNAME=immich-redis",
        "IMMICH_MACHINE_LEARNING_URL=http://immich-machine-learning:3003"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/immich/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/immich/DATA/AppData/$AppID/upload",
          "target": "/usr/src/app/upload",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:2283",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
