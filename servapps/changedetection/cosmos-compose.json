{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Change Detection",
          "description": "Web UI",
          "useHost": true,
          "target": "http://changedetection:5000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "changedetection": {
  "container_name": "big-bear-changedetection",
  "image": "ghcr.io/dgtlmoon/changedetection.io:0.50.32",
  "restart": "unless-stopped",
  "ports": [
    "5000:5000"
  ],
  "volumes": [
    "changedetection_datastore:/datastore"
  ]
}
    }
  }
}
