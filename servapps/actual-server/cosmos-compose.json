{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Actual Budget",
          "description": "Web UI",
          "useHost": true,
          "target": "http://actual-server:5006",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "actual-server": {
  "image": "actualbudget/actual-server:25.12.0",
  "container_name": "actual-server",
  "volumes": [
    "big-bear-actual-server_data:/data"
  ],
  "ports": [
    "5006:5006"
  ],
  "restart": "unless-stopped"
}
    }
  }
}
