{
  "cosmos-installer": {
    "routes": [
        {
          "name": "OctoPrint",
          "description": "Web UI",
          "useHost": true,
          "target": "http://octoprint:1080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "octoprint": {
  "container_name": "octoprint",
  "image": "octoprint/octoprint:1.11.3",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-octoprint_octoprint:/octoprint"
  ],
  "ports": [
    "1080:80"
  ]
}
    }
  }
}
