{
  "cosmos-installer": {
    "routes": [
        {
          "name": "NPMPlus",
          "description": "Web UI",
          "useHost": true,
          "target": "http://npmplus:81",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "npmplus": {
  "container_name": "big-bear-npmplus",
  "image": "zoeyvid/npmplus:449",
  "restart": "unless-stopped",
  "network_mode": "host",
  "environment": [
    "TZ=America/Chicago"
  ],
  "volumes": [
    "npmplus_data:/data"
  ]
}
    }
  }
}
