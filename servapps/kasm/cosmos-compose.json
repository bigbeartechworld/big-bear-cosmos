{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "linuxserver/kasm:1.120.20221218",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "KASM_PORT=443"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/kasm/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/kasm/DATA/AppData/$AppID/opt",
          "target": "/opt",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/kasm/DATA/AppData/$AppID/profiles",
          "target": "/profiles",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/kasm/dev/input",
          "target": "/dev/input",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/kasm/run/udev/data",
          "target": "/run/udev/data",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:3000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
