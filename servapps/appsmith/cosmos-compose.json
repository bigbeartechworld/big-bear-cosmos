{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Appsmith",
          "description": "Web UI",
          "useHost": true,
          "target": "http://appsmith:1080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "appsmith": {
  "image": "appsmith/appsmith-ce:v1.73.1",
  "container_name": "appsmith",
  "ports": [
    "1080:80",
    "10443:443"
  ],
  "volumes": [
    "big-bear-appsmith_stacks:/appsmith-stacks"
  ],
  "restart": "unless-stopped"
}
    }
  }
}
