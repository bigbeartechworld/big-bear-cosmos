{
  "cosmos-installer": {
    "routes": [
        {
          "name": "FarmOS",
          "description": "Web UI",
          "useHost": true,
          "target": "http://farmos:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "farmos": {
  "container_name": "big-bear-farmos",
  "image": "farmos/farmos:3.4.5",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-farmos_sites:/opt/drupal/web/sites"
  ],
  "ports": [
    "8080:80"
  ],
  "networks": [
    "big-bear-farmos-network"
  ]
}
    }
  }
}
