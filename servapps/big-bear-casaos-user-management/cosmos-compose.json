{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "bigbeartechworld/big-bear-casaos-user-management:0.1.1",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "FLASK_APP=app.py",
        "FLASK_ENV=production",
        "ADMIN_USERNAME=casaos",
        "ADMIN_PASSWORD=casaos"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/big-bear-casaos-user-management/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/big-bear-casaos-user-management/sys/fs/cgroup:/sys/fs/cgroup",
          "target": "ro",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/big-bear-casaos-user-management/var/lib/casaos/db",
          "target": "/var/lib/casaos/db",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/big-bear-casaos-user-management/run/systemd/system",
          "target": "/run/systemd/system",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/big-bear-casaos-user-management/var/run/dbus/system_bus_socket",
          "target": "/var/run/dbus/system_bus_socket",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:5000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
