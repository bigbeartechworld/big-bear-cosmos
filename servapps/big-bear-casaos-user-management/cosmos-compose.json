{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Big Bear CasaOS User Management",
          "description": "Web UI",
          "useHost": true,
          "target": "http://big-bear-casaos-user-management:5000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "big-bear-casaos-user-management": {
  "container_name": "big-bear-casaos-user-management",
  "image": "bigbeartechworld/big-bear-casaos-user-management:0.1.1",
  "restart": "unless-stopped",
  "privileged": true,
  "environment": [
    "FLASK_APP=app.py",
    "FLASK_ENV=production",
    "ADMIN_USERNAME=casaos",
    "ADMIN_PASSWORD=casaos"
  ],
  "volumes": [
    "/sys/fs/cgroup:/sys/fs/cgroup:ro",
    "/var/lib/casaos/db:/var/lib/casaos/db",
    "/run/systemd/system:/run/systemd/system",
    "/var/run/dbus/system_bus_socket:/var/run/dbus/system_bus_socket"
  ],
  "ports": [
    "5000:5000"
  ],
  "cap_add": [
    "SYS_ADMIN"
  ],
  "security_opt": [
    "seccomp:unconfined"
  ]
}
    }
  }
}
