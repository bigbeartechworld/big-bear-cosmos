{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Obsidian",
          "description": "Web UI",
          "useHost": true,
          "target": "http://obsidian:3000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "obsidian": {
  "container_name": "big-bear-obsidian",
  "image": "linuxserver/obsidian:1.9.14",
  "restart": "unless-stopped",
  "environment": [
    "PUID=1000",
    "PGID=1000",
    "TZ=Etc/UTC"
  ],
  "volumes": [
    "obsidian_config:/config"
  ],
  "ports": [
    "3000:3000",
    "3001:3001"
  ]
}
    }
  }
}
