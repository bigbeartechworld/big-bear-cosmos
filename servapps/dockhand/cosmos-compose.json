{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Dockhand",
          "description": "Web UI",
          "useHost": true,
          "target": "http://dockhand:3003",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "dockhand": {
  "container_name": "big-bear-dockhand",
  "image": "fnsys/dockhand:latest",
  "restart": "unless-stopped",
  "volumes": [
    "/var/run/docker.sock:/var/run/docker.sock",
    "big-bear-dockhand_data:/app/data"
  ],
  "ports": [
    "3003:3000"
  ]
}
    }
  }
}
