{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Compose Toolbox",
          "description": "Web UI",
          "useHost": true,
          "target": "http://composetoolbox:3002",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "composetoolbox": {
  "environment": {
    "NODE_ENV": "production"
  },
  "image": "ghcr.io/bluegoosemedia/composetoolbox:latest",
  "pull_policy": "always",
  "ports": [
    "3002:3000"
  ],
  "restart": "unless-stopped",
  "volumes": [
    "composetoolbox_data:/app/data"
  ],
  "container_name": "big-bear-composetoolbox"
}
    }
  }
}
