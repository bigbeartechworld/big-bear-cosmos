{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Invoice Ninja",
          "description": "Web UI",
          "useHost": true,
          "target": "http://invoice-ninja:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "invoice-ninja": {
  "container_name": "big-bear-invoice-ninja-web",
  "image": "nginx:1.29",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-invoice-ninja_data_nginx_invoice-ninja.conf:/etc/nginx/conf.d/default.conf:ro",
    "big-bear-invoice-ninja_data_public:/var/www/app/public:ro"
  ],
  "ports": [
    "8080:80"
  ],
  "networks": [
    "big-bear-invoice-ninja-network"
  ]
}
    }
  }
}
