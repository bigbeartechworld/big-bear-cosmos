{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "registry.gitlab.com/timvisee/send:v3.4.27",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "NODE_ENV=production",
        "BASE_URL=http://${APP_DOMAIN}",
        "REDIS_HOST=big-bear-send-visee-redis",
        "MAX_FILE_SIZE=2147483648",
        "FILE_DIR=/uploads"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/send-visee/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/send-visee/DATA/AppData/$AppID/uploads",
          "target": "/uploads",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:1443",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
