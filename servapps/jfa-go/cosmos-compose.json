{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "hrfee/jfa-go:latest",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/jfa-go/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/jfa-go/DATA/AppData/$AppID/.config/jfa-go",
          "target": "/data",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/jfa-go/DATA/AppData/$AppID/jellyfin",
          "target": "/jf",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/jfa-go/etc/localtime:/etc/localtime",
          "target": "ro",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:8056",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
