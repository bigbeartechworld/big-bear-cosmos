{
  "cosmos-installer": {
    "routes": [
        {
          "name": "jfa-go",
          "description": "Web UI",
          "useHost": true,
          "target": "http://jfa-go:8056",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "jfa-go": {
  "container_name": "big-bear-jfa-go",
  "image": "hrfee/jfa-go:latest",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-jfa-go_.config_jfa-go:/data",
    "big-bear-jfa-go_jellyfin:/jf",
    "/etc/localtime:/etc/localtime:ro"
  ],
  "ports": [
    "8056:8056"
  ]
}
    }
  }
}
