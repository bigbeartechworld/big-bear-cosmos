{
  "cosmos-installer": {
    "routes": [
        {
          "name": "SFTPGO",
          "description": "Web UI",
          "useHost": true,
          "target": "http://sftpgo:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "sftpgo": {
  "container_name": "big-bear-sftpgo",
  "image": "drakkan/sftpgo:v2.7",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-sftpgo_srv:/srv/sftpgo",
    "big-bear-sftpgo_data:/var/lib/sftpgo"
  ],
  "ports": [
    "8080:8080",
    "2022:2022",
    "2121:2121",
    "10080:10080"
  ],
  "environment": [
    "SFTPGO_HTTPD_BINDINGS_0_PORT=8080",
    "SFTPGO_HTTPD_BINDINGS_0_ADDRESS=0.0.0.0",
    "SFTPGO_DATA_PROVIDER_CREATE_DEFAULT_ADMIN=true",
    "SFTPGO_WEBDAVD_BINDINGS_0_PORT=10080",
    "SFTPGO_FTPD_BINDINGS_0_PORT=2121",
    "SFTPGO_FTPD_BINDINGS_0_FORCE_PASSIVE_IP='127.0.0.1:8080'"
  ]
}
    }
  }
}
