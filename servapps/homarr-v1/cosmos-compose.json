{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Homarr v1",
          "description": "Web UI",
          "useHost": true,
          "target": "http://homarr-v1:7575",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "homarr-v1": {
  "container_name": "big-bear-homarr-v1",
  "image": "ghcr.io/homarr-labs/homarr:v1.43.0",
  "restart": "unless-stopped",
  "volumes": [
    "/var/run/docker.sock:/var/run/docker.sock",
    "big-bear-homarr-v1_appdata:/appdata"
  ],
  "environment": [
    "SECRET_ENCRYPTION_KEY="
  ],
  "ports": [
    "7575:7575"
  ]
}
    }
  }
}
