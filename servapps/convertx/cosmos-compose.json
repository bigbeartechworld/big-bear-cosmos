{
  "cosmos-installer": {
    "routes": [
        {
          "name": "ConvertX",
          "description": "Web UI",
          "useHost": true,
          "target": "http://convertx:3000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "convertx": {
  "container_name": "big-bear-convertx",
  "image": "ghcr.io/c4illin/convertx:v0.15.1",
  "restart": "unless-stopped",
  "ports": [
    "3000:3000"
  ],
  "environment": [
    "JWT_SECRET=c8330437-e6ae-4cf0-9837-4d1326457e32",
    "HTTP_ALLOWED=true"
  ],
  "volumes": [
    "big-bear-convertx_data:/app/data"
  ]
}
    }
  }
}
