{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Nexterm",
          "description": "Web UI",
          "useHost": true,
          "target": "http://nexterm:6989",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "nexterm": {
  "container_name": "big-bear-nexterm",
  "image": "germannewsmaker/nexterm:1.0.6-OPEN-PREVIEW",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-nexterm_data:/app/data"
  ],
  "ports": [
    "6989:6989"
  ]
}
    }
  }
}
