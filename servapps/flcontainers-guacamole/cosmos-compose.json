{
  "cosmos-installer": {
    "routes": [
        {
          "name": "FlContainers Guacamole",
          "description": "Web UI",
          "useHost": true,
          "target": "http://flcontainers-guacamole:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "flcontainers-guacamole": {
  "image": "flcontainers/guacamole:1.6.0",
  "container_name": "big-bear-flcontainers-guacamole",
  "restart": "always",
  "environment": {
    "tz": "UTC",
    "EXTENSIONS": "auth-totp,auth-ldap"
  },
  "ports": [
    "8080:8080"
  ]
}
    }
  }
}
