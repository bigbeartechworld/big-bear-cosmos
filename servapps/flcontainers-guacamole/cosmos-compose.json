{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "flcontainers/guacamole:1.6.0",
      "container_name": "{ServiceName}",
      "restart": "always",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "tz=UTC",
        "EXTENSIONS=auth-totp,auth-ldap"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/flcontainers-guacamole/icon.png"
      },
      "volumes": [
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
