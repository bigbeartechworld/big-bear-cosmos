{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "ghcr.io/tchapi/davis-standalone:5.2.0",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "APP_ENV=prod",
        "DATABASE_DRIVER=mysql",
        "DATABASE_URL=mysql://bigbear:0c9d2acc-d4f3-423c-8361-86f35cdd3eb2@big-bear-davis-mysql:3306/davis?serverVersion=10.6.10&charset=utf8mb4",
        "MAILER_DSN=smtp://:@big-bear-davis-mailpit:1025",
        "ADMIN_LOGIN=bigbear",
        "ADMIN_PASSWORD=aa9405b8-426c-41a1-bbc6-2924d8ec7eb3",
        "AUTH_REALM=",
        "AUTH_METHOD=",
        "CALDAV_ENABLED=",
        "CARDDAV_ENABLED=",
        "WEBDAV_ENABLED=",
        "WEBDAV_TMP_DIR=",
        "WEBDAV_PUBLIC_DIR=",
        "WEBDAV_HOMES_DIR=",
        "INVITE_FROM_ADDRESS=",
        "APP_TIMEZONE=UTC"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/davis/icon.png"
      },
      "volumes": [
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:9000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
