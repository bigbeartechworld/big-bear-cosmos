{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Chatpad",
          "description": "Web UI",
          "useHost": true,
          "target": "http://chatpad:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "chatpad": {
  "container_name": "big-bear-chatpad",
  "image": "ghcr.io/deiucanta/chatpad:f45cd53bc410412610c1ba1cbd84cd137d8e167d",
  "restart": "unless-stopped",
  "ports": [
    "8080:80"
  ]
}
    }
  }
}
