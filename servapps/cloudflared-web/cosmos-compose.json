{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Cloudflared Web",
          "description": "Web UI",
          "useHost": true,
          "target": "http://cloudflared-web:14333",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "cloudflared-web": {
  "container_name": "big-bear-cloudflared-web",
  "image": "wisdomsky/cloudflared-web:2025.10.0",
  "restart": "unless-stopped",
  "network_mode": "host",
  "environment": [
    "WEBUI_PORT=14333",
    "EDGE_IP_VERSION=auto",
    "PROTOCOL=auto",
    "METRICS_ENABLE=false",
    "METRICS_PORT=60123",
    "BASIC_AUTH_USER=admin",
    "BASIC_AUTH_PASS=\"\""
  ]
}
    }
  }
}
