{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Neko Firefox",
          "description": "Web UI",
          "useHost": true,
          "target": "http://neko-firefox:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "neko-firefox": {
  "container_name": "neko-firefox",
  "image": "m1k1o/neko:firefox",
  "restart": "unless-stopped",
  "shm_size": "2gb",
  "ports": [
    "8080:8080",
    "52000:52000/udp",
    "52001:52001/udp",
    "52002:52002/udp",
    "52003:52003/udp",
    "52004:52004/udp",
    "52005:52005/udp",
    "52006:52006/udp",
    "52007:52007/udp",
    "52008:52008/udp",
    "52009:52009/udp",
    "52010:52010/udp"
  ],
  "environment": {
    "NEKO_SCREEN": "1920x1080@30",
    "NEKO_PASSWORD": "neko",
    "NEKO_PASSWORD_ADMIN": "bigbear",
    "NEKO_EPR": "52000-52010",
    "NEKO_ICELITE": "1",
    "NEKO_DEBUG": "1",
    "NEKO_NAT1TO1": "[CASAOS_IP]"
  }
}
    }
  }
}
