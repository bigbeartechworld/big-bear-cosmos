{
  "cosmos-installer": {
    "routes": [
        {
          "name": "CyberChef",
          "description": "Web UI",
          "useHost": true,
          "target": "http://cyberchef:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "cyberchef": {
  "container_name": "big-bear-cyberchef",
  "image": "ghcr.io/gchq/cyberchef:10.22.0",
  "restart": "unless-stopped",
  "ports": [
    "8080:80"
  ]
}
    }
  }
}
