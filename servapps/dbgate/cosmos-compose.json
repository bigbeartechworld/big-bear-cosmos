{
  "cosmos-installer": {
    "routes": [
        {
          "name": "DBGate",
          "description": "Web UI",
          "useHost": true,
          "target": "http://dbgate:3000",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "dbgate": {
  "container_name": "big-bear-dbgate",
  "image": "dbgate/dbgate:6.6.6-alpine",
  "restart": "unless-stopped",
  "volumes": [
    "big-bear-dbgate_config:/root/.dbgate"
  ],
  "ports": [
    "3000:3000"
  ]
}
    }
  }
}
