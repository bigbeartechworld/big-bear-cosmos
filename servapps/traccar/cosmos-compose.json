{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "traccar/traccar:6.10-ubuntu",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/traccar/icon.png"
      },
      "volumes": [
        {
          "source": "{DefaultDataPath}/traccar/DATA/AppData/$AppID/logs:/opt/traccar/logs",
          "target": "rw",
          "type": "bind"
        },
        {
          "source": "{DefaultDataPath}/traccar/DATA/AppData/$AppID/traccar.xml:/opt/traccar/conf/traccar.xml",
          "target": "ro",
          "type": "bind"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:8082",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
