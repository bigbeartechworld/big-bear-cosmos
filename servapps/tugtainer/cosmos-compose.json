{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Tugtainer",
          "description": "Web UI",
          "useHost": true,
          "target": "http://tugtainer:9412",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "tugtainer": {
  "container_name": "big-bear-tugtainer",
  "image": "quenary/tugtainer:1.12.0",
  "restart": "unless-stopped",
  "ports": [
    "9412:80"
  ],
  "environment": {
    "TZ": "UTC",
    "LOG_LEVEL": "warning",
    "JWT_SECRET_KEY": "your-secret-key-here",
    "ACCESS_TOKEN_LIFETIME_MIN": "5",
    "REFRESH_TOKEN_LIFETIME_MIN": "43200",
    "HTTPS": "FALSE"
  },
  "volumes": [
    "big-bear-tugtainer_data:/tugtainer",
    "/var/run/docker.sock:/var/run/docker.sock"
  ]
}
    }
  }
}
