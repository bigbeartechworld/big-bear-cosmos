{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Gotify",
          "description": "Web UI",
          "useHost": true,
          "target": "http://gotify:8091",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "gotify": {
  "image": "gotify/server:2.8.0",
  "ports": [
    "8091:80"
  ],
  "environment": [
    "GOTIFY_DEFAULTUSER_NAME=admin",
    "GOTIFY_DEFAULTUSER_PASS=custom"
  ],
  "volumes": [
    "gotify_data:/app/data"
  ],
  "network_mode": "bridge"
}
    }
  }
}
