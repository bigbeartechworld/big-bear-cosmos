{
  "cosmos-installer": {},
  "minVersion": "0.7.6",
  "services": {
    "{ServiceName}": {
      "image": "phpmyadmin/phpmyadmin:5.2.3",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "environment": [
        "PMA_HOST=[YOUR-CASAOS-IP]",
        "PMA_PORT=3306",
        "PMA_USER=bigbear",
        "PMA_PASSWORD=password"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/phpmyadmin/icon.png"
      },
      "volumes": [
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:80",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
