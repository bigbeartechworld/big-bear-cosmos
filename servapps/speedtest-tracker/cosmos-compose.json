{
  "cosmos-installer": {
    "routes": [
        {
          "name": "Speedtest Tracker",
          "description": "Web UI",
          "useHost": true,
          "target": "http://speedtest-tracker:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "BlockAPIAbuse": true
        }
      ],
    "services": {
      "speedtest-tracker": {
  "container_name": "big-bear-speedtest-tracker",
  "ports": [
    "8080:80",
    "8443:443"
  ],
  "environment": [
    "PUID=1000",
    "PGID=1000",
    "DB_CONNECTION=mysql",
    "DB_HOST=big-bear-speedtest-tracker-db",
    "DB_PORT=3306",
    "DB_DATABASE=speedtest_tracker",
    "DB_USERNAME=casaos",
    "DB_PASSWORD=casaos",
    "TZ=America/Chicago"
  ],
  "volumes": [
    "/etc/localtime:/etc/localtime:ro",
    "speedtest-tracker_config:/config",
    "speedtest-tracker_web:/etc/ssl/web"
  ],
  "image": "linuxserver/speedtest-tracker:1.7.0",
  "restart": "unless-stopped",
  "depends_on": [
    "big-bear-speedtest-tracker-db"
  ],
  "networks": [
    "big_bear_speedtest_tracker_network"
  ]
}
    }
  }
}
